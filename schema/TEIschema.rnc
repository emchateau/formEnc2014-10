tei_model.rdgLike = tei_rdg
tei_app =
  
  ##  (entrée d'apparat critique) contient une entrée dans un apparat critique,
  ##			constituée d'un lemme facultatif et d'au moins une leçon. [12.1.1. ]
  element app {
    (tei_model.global*,
     (tei_lem, tei_model.global*, (tei_wit, tei_model.global*)?)?,
     ((tei_model.rdgLike,
       tei_model.global*,
       (tei_wit, tei_model.global*)?)
      | (tei_rdgGrp,
         tei_model.global*,
         (tei_wit, tei_model.global*)?))*)
    >> sch:pattern [
         id = "app-constraint-only1lem"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:app"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "count( descendant::tei:lem[ generate-id(    current() ) = generate-id(    ancestor::tei:app[1] ) ]) <       2"
             "\x{a}" ~
             "Only one <lem> element may appear within a single  apparatus\x{a}" ~
             "entry, whether it appears outside a <rdgGrp>\x{a}" ~
             "element or within it."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    
    ## classifie la variation contenue dans cet
    ##					élément selon toute typologie adéquate.
    attribute type { tei_data.enumerated }?,
    
    ## identifie, si nécessaire, le début du
    ##					lemme dans le texte de base.
    attribute from { tei_data.pointer }?,
    
    ## identifie, si nécessaire, la fin du lemme
    ##					dans le texte de base.
    attribute to { tei_data.pointer }?,
    
    ##  (emplacement) indique la localisation de la variante
    ##					lorsqu'on utilise dans l'encodage de l'apparat critique une méthode de
    ##					référencement des localisations.
    attribute loc {
      list { tei_data.word+ }
    }?,
    empty
  }
