  element witDetail {
    tei_macro.phraseSeq,
    tei_att.global.attributes,
    tei_att.placement.attributes,
    tei_att.responsibility.attributes,
    tei_att.pointing.attributes,
    
    ##  (témoins) contient le ou les code(s)				identifiant le ou le(s) témoin(s) auxquels fait référence l'élément
    ##						witDetail.
    attribute wit {
      list { tei_data.pointer+ }
    },
    
    ## décrit le type de renseignement donné sur
    ##					le témoin.
    attribute type { tei_data.enumerated }?,
